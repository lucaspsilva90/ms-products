{

  "version": "0.2.0",
  "configurations": [
      {
          "type": "node",
          "request": "launch",
            "name": "Launch NestJS",
          "skipFiles": [
              "<node_internals>/**"
          ],
          "program": "${workspaceFolder}/dist/src/infra/main.js",
          "preLaunchTask": "tsc: build - tsconfig.json",
          "outFiles": [
              "${workspaceFolder}/dist/**/**/*.js"
          ],
          "env": {
              "NODE_ENV": "development"
          },
          "console": "integratedTerminal",
          "runtimeVersion": "18"
      }
  ]
}
